<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.equinox.http.registry.resources">
      <resource
            alias="/images"
            base-name="/resources">
      </resource>
      <resource
            alias="/"
            base-name="/html">
      </resource>
   </extension>
   <extension point="org.eclipse.equinox.http.registry.servlets">
	<servlet alias="/helloworld" 
	         class="org.eclipse.equinox.jsp.jasper.registry.JSPFactory:/jsp/hello.jsp"/>
   </extension>
</plugin>
